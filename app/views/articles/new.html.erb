<h1>New Article</h1>

<%= form_for @article do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit "Add a new article" %>
  </p>
<% end %>

<%= link_to "Back to All articles", articles_path %>
