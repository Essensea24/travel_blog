Edit your post! <%= @article.title%>

<%= form_for @article do |f| %>
	<div>
		<%= f.label :title %>
		<%= f.text_field :title %>
	</div>
	<div>
		<%= f.label :text %>
		<%= f.text_field :text %>
	</div>

	<%= f.submit "Make this change!" %>
<% end %>

<%= link_to "Back to all articles", articles_path %>